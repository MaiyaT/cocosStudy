{
    "version": "1.4.0",
    "configurations": [
        {
            "name": "Creator Debug: cocos",
            "type": "cocos",
            "request": "launch",
            "url": "http://localhost:7456",
            "sourceMaps": true,
            "userDataDir": "${workspaceRoot}/.vscode/cocos",
            "diagnosticLogging": false,
            "sourceMapPathOverrides": {
                "assets/*": "${workspaceRoot}/assets/*"
            },
            "webRoot": "${workspaceRoot}"
        }
    ]
}